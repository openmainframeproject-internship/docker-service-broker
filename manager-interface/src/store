import Vue from 'vue'
import axios from 'axios'
import VueAxios from 'vue-axios'

Vue.use(VueAxios, axios)

export const store = {
  state: {
    programs: []
  },
  getprograms () {
    const getUrl = 'https://148.100.99.53:80'
    axios.get(getUrl).then((response) => {
      this.state.programs = response.data
    })
  }
}